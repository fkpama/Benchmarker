<?xml version="1.0" encoding="utf-8"?>
<Project>
		<PropertyGroup>
				<LangVersion>latest</LangVersion>
				<AssemblyName Condition="'$(AssemblyName)'==''">Benchmarker.$(MSBuildProjectName)</AssemblyName>
		</PropertyGroup>

		<PropertyGroup>
				<NpmPackageProjectDirectory Condition="'$(NpmPackageProjectDirectory)'==''">$([MSBuild]::NormalizePath('$(MSBuildThisFileDirectory)src\DevOps\Package'))</NpmPackageProjectDirectory>
		</PropertyGroup>

		<PropertyGroup>
				<Authors>Sodiware</Authors>
				<!--<PackageProjectUrl>https://github.com/fkpama/Benchmarker</PackageProjectUrl>-->

				<EmbedUntrackedSources>true</EmbedUntrackedSources>

				<!-- Deterministic dll. See https://learn.microsoft.com/en-us/dotnet/core/project-sdk/msbuild-props#continuousintegrationbuild -->
				<ContinuousIntegrationBuild Condition="'$(BUILD_BUILDID)'!=''">true</ContinuousIntegrationBuild>

				<IncludeSymbols>true</IncludeSymbols>
				<SymbolPackageFormat>snupkg</SymbolPackageFormat>
				<PublishRepositoryUrl>true</PublishRepositoryUrl>
				<AllowedOutputExtensionsInPackageBuildOutputFolder Condition="'$(Configuration)'=='Debug'">$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
		</PropertyGroup>

		<PropertyGroup>
			<PackageOutputPath Condition="'$(PackageOutputPath)'==''">$(MSBuildThisFileDirectory)tests/PkgRepository</PackageOutputPath>
		</PropertyGroup>

		<ItemGroup Condition="'$(Configuration)'=='Debug'">
				<InternalsVisibleTo Include="Benchmarker.FrameworkTests, " />
		</ItemGroup>

</Project>
